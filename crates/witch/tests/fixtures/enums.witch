enum Foobar {
    Foo,
    Bar,
    Baz(usize)
}

enum Option[T] {
    Some(T),
    None
}

let x = Some("hello")

function has_string(maybe_string: Option[Foobar]) -> string {
    case maybe_string {
        Some(Foo) -> {
            return "foo"
        }
        Some(Bar) -> {
            return "bar"
        }
        Some(Baz(num)) -> {
            return num
        }
        None -> { return "fail" }
    }

    return "fail"
}

has_string(x)