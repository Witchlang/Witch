enum Foobar {
    Foo,
    Bar,
    Baz(usize)
}

enum Option[T] {
    Some(T),
    None
}

let x = Some("hello")

function has_string(maybe_string: Option[string]) -> string {
    case maybe_string {
        Some("foo") -> {
            return "foo"
        }
        Some(str) -> {
            return str
        }
        None -> { return "fail" }
    }

    return "fail"
}

has_string(x)