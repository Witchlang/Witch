enum Foobar {
    Foo,
    Bar,
    Baz
}

enum Option[T] {
    Some(T),
    None
}

let x = Some("Foo")

function has_string(maybe_string: Option[string]) -> string {
    return case maybe_string {
        Some(x) -> {
            return "bajs"
        }
        None -> { return "none" }
    }
}

has_string(None)