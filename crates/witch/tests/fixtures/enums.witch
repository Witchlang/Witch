enum Lorem {
    Ipsum,
    Dolor
}

enum Foobar {
    Foo,
    Bar,
    Baz(Lorem)
}

enum Option[T] {
    Some(T),
    None
}

let x = Some(Bar)

function has_string(maybe_string: Option[Foobar]) -> string {
    return case maybe_string {
        Some(Foo) -> {
            return "foo"
        } 
        Some(Bar) -> {
            return "bajs"
        }
        Some(Baz(Ipsum)) -> {
            return "bajs"
        }
        Some(Baz(x)) -> {
            return "bajs"
        }
        None -> { return "none" }
    }
}

has_string(x)